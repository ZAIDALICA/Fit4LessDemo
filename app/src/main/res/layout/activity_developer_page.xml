<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeveloperPage">


    <Button
        android:id="@+id/bt_db"
        style="@style/buttonStyle"
        android:layout_width="293dp"
        android:layout_height="108dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/button_shape"
        android:onClick="goToDb"
        android:text="DB ALL USERS"
        android:textColor="#CDDC39"
        android:textSize="24sp"
        app:backgroundTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMyBooking"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnMyBooking"
        style="@style/buttonStyle"
        android:layout_width="297dp"
        android:layout_height="111dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="130dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="128dp"
        android:layout_marginRight="128dp"
        android:background="@drawable/button_shape"
        android:onClick="btnMyAppointmentsClicked"
        android:text="DB ALL BOOKINGS"
        android:textColor="#CDDC39"
        android:textSize="24sp"
        app:backgroundTint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>